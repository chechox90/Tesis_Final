                        optionMa1.val(data.data[i]["ID_TERMINAL"]);
                        optionMa1.text(data.data[i]["NOMBRE_TERMINAL"]);
                        optionMi1.val(data.data[i]["ID_TERMINAL"]);
                        optionMi1.text(data.data[i]["NOMBRE_TERMINAL"]);
